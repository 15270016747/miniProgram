<!--pages/home/home.wxml-->
<include src="/wxml/header.wxml"/>
<swiper 
  circular
  autoplay
  indicator-dots
  indicator-color="#ff5777" 
  indicator-active-color="#fff"
  class="home-swiper">
  <swiper-item wx:for="{{banner}}" wx:key="acm" class="home-swiper-item">
    <image src="{{item.image}}"></image>
  </swiper-item>
</swiper>
<view style="height:50px;line-height:50px" class="{{isActive? 'active':''}}">{{nowTime}}</view>
<button style="display:inline-block" bindtap="toRed">change</button>
<block wx:for="{{9}}" wx:for-item="num" wx:key="item">
  <view>{{num}}</view>
</block>
<wxs module="info"  src="../../wxs/info.wxs" />
