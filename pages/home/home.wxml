<!--pages/home/home.wxml-->
<!-- <include src="/wxml/header.wxml"/> -->
<home-swiper wx:if="{{isCreate}}" banner="{{banner}}"/>
<tab-control bind:handletagclick="handletagclick" titleList="{{['流行','新款','精选']}}" id="tab"/>
<my-slot>
  <view slot="center">aaa</view>
</my-slot>
<my-con bind:change="handleChange" title="按我一下" titleClass="red"/>
<view>count:{{count}}</view>

<view style="height:50px;line-height:50px" class="{{isActive? 'active':''}}">{{nowTime}}</view>
<button bind:touchend="handleTouchmove" style="display:inline-block" bind:tap="toRed">change</button>
<block wx:for="{{9}}" wx:for-item="num" wx:key="item">
  <view>{{num}}</view>
</block>
<slider value="40"></slider>
<wxs module="info"  src="../../wxs/info.wxs" />
